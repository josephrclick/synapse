# Development Environment Settings
# This file overrides base .env settings for development

# Logging
LOG_LEVEL=DEBUG

# Ollama Configuration - Local development
OLLAMA_BASE_URL=http://host.docker.internal:11434

# Chunking Configuration - Smaller chunks for testing
CHUNK_SPLIT_LENGTH=3

# Model Configuration
GENERATIVE_MODEL=gemma3n:e2b

# API Security
INTERNAL_API_KEY=test-api-key-123
BACKEND_API_KEY=test-api-key-123

DATABASE_URL=postgresql://localhost:5432/synapse_db
ENVIRONMENT=development
USE_LOCAL_DB=true

# EXPLICITLY EMPTY - Forces developers to set up local
SUPABASE_URL=
SUPABASE_SERVICE_KEY=